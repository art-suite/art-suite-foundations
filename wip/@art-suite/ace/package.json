{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bin": {
    "ace": "bin/ace"
  },
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-browser-tools": "^1.2.12",
    "art-build-configurator": "^1.29.5",
    "art-class-system": "^1.11.15",
    "art-config": "^2.0.3",
    "art-filebuilder": "^2.1.13",
    "art-object-tree-factory": "^2.2.10",
    "art-standard-lib": "^1.71.2",
    "bluebird": "^3.5.5",
    "caffeine-script": "^0.74.0",
    "caffeine-script-runtime": "^1.14.0",
    "coffee-script": "^1.12.7",
    "colors": "^1.4.0",
    "commander": "^5.0.0",
    "dateformat": "^4.5.1",
    "detect-node": "^2.0.4",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^4.1.0",
    "neptune-namespaces": "^4.2.11",
    "simple-git": "^2.31.0"
  },
  "description": "Tools for configuring npm (package.json) and webpack (webpack.config.js)",
  "devDependencies": {
    "art-testbench": "^3.2.4",
    "jest": "^27.0.6",
    "mocha": "^7.2.0",
    "mock-fs": "^4.13.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/_?[Jjt]est/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "temp",
      "source/test",
      "source-source"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "@art-suite/configuration-engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "1.26.21",
  "watch": {
    "build": {
      "extensions": [
        "caf"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
