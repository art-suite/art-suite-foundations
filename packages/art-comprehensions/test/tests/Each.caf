import &StandardImport

suite: ->

  test "each null", ->
    count = 0
    _each null, (v, k) -> count++
    assert.eq 0, count

  test "each [1,2,3]", ->
    count = 0
    _each [1,2,3], (v, k) -> count++
    assert.eq 3, count

  test "each {a:1,b:2,c:3}", ->
    count = 0
    _each {a:1,b:2,c:3}, (v, k) -> count++
    assert.eq 3, count
