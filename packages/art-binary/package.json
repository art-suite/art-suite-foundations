{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-class-system": "^1.11.14",
    "art-standard-lib": "^1.71.1"
  },
  "description": "Art.Binary",
  "devDependencies": {
    "art-build-configurator": "^1.29.4",
    "art-testbench": "^3.2.3",
    "mocha": "^7.2.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "build/**/*.js",
      "!build/test/**/*.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test"
    ],
    "verbose": true
  },
  "license": "ISC",
  "name": "art-binary",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee -m --output build --compile source &\nCOFFEE_PID=$!\ncaf -m -c source -o build &\nCAF_PID=$!\nwait $COFFEE_PID || exit 1\nwait $CAF_PID || exit 1\nnpm run nn",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf build/*",
    "nn": "nn build/* test/tests",
    "test": "npm run nn\nmocha -u tdd",
    "watch": "npm-watch"
  },
  "version": "1.1.15",
  "watch": {
    "build": {
      "extensions": [
        "caf",
        "coffee"
      ],
      "patterns": [
        "source"
      ],
      "quiet": false
    }
  }
}
