{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https:/github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-class-system": "*",
    "art-standard-lib": "*"
  },
  "description": "ArtSuite.SourceRootFinder",
  "devDependencies": {
    "art-build-configurator": "*",
    "art-testbench": "*",
    "jest": "*",
    "npm-watch": "^0.6.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/_?[Jjt]est/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "@art-suite/source-root-finder",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "1.0.0",
  "watch": {
    "build": {
      "extensions": [
        "caf"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
