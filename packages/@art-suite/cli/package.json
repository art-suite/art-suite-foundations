{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "ansi-wordwrap": "^1.0.2",
    "art-class-system": "^1.11.14",
    "art-standard-lib": "^1.70.13",
    "colors": "^1.4.0"
  },
  "description": "Art.Cli",
  "devDependencies": {
    "art-build-configurator": "^1.29.4",
    "art-testbench": "^3.2.3",
    "mocha": "^7.2.0",
    "mock-fs": "^4.13.0",
    "npm-watch": "^0.9.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/test/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "@art-suite/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "caf -m -c source-source -o source\nnpm run nn",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "nn": "nn source/* ",
    "test": "npm run nn\njest",
    "watch": "npm-watch"
  },
  "version": "1.3.13",
  "watch": {
    "build": {
      "extensions": [
        "caf"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
