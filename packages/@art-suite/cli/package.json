  "dependencies": {
    "ansi-wordwrap": "^1.0.2",
    "art-class-system": "^1.11.12",
    "art-standard-lib": "^1.70.5",
    "colors": "^1.4.0"
  },
  "description": "Art.Cli",
  "devDependencies": {
    "art-build-configurator": "^1.29.2",
    "art-testbench": "^3.2.1",
    "mocha": "^7.2.0",
    "mock-fs": "^4.13.0",
    "npm-watch": "^0.7.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "@art-suite/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "1.3.10",
  "watch": {
    "build": {
      "extensions": "caf",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
