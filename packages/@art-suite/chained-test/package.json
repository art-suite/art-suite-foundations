{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {},
  "description": "@art-suite/chained-test lets you break up large Jest or Mocha tests into a sequences of smaller tests. This is particularly helpful for integration tests, user-story tests, and tests with more than one asynchronous step.",
  "devDependencies": {
    "@art-suite/assert": "^1.0.3",
    "art-build-configurator": "^1.29.3",
    "jest": "^26.6.1"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/test/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "@art-suite/chained-test",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "caf -m -c source-source -o source\nnn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "1.3.1",
  "watch": {
    "build": {
      "extensions": [
        "caf"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
