{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-class-system": "^1.11.13",
    "art-standard-lib": "^1.70.6"
  },
  "description": "Art.Epoch",
  "devDependencies": {
    "art-build-configurator": "^1.29.3",
    "art-testbench": "^3.2.2"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/test/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "art-epoched-state",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee -m --output source --compile source-source &\nCOFFEE_PID=$!\ncaf -m -c source-source -o source &\nCAF_PID=$!\nwait $COFFEE_PID || exit 1\nwait $CAF_PID || exit 1\nnn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "mocha -u tdd",
    "watch": "npm-watch"
  },
  "version": "1.2.12",
  "watch": {
    "build": {
      "extensions": [
        "caf",
        "coffee"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
