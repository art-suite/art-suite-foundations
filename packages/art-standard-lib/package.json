{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "bluebird": "^3.5.5",
    "caffeine-script-runtime": "^1.14.0",
    "dateformat": "^3.0.3",
    "detect-node": "^2.0.4",
    "neptune-namespaces-runtime": "^3.2.9",
    "pluralize": "^8.0.0"
  },
  "description": "The Standard Library for JavaScript that aught to be.",
  "devDependencies": {
    "art-build-configurator": "^1.29.3",
    "art-testbench": "^3.2.2",
    "mocha": "^7.2.0",
    "mock-fs": "^4.13.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/_?[Jjt]est/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "art-standard-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee --output source --compile source-source; caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "nn -s;mocha -u tdd",
    "watch": "npm-watch"
  },
  "version": "1.70.11",
  "watch": {
    "build": {
      "extensions": [
        "caf",
        "coffee"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
