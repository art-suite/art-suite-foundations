{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-build-configurator": "^1.26.14"
  },
  "description": "Create Declarative frameworks that output tree structures with properties (e.g. ArtReact)",
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "license": "ISC",
  "name": "art-object-tree-factory",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "build": "caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "2.2.1",
  "watch": {
    "build": {
      "extensions": "caf",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
