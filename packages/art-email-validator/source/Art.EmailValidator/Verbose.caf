import &ArtStandardLib
logResult: (email, result, context) ->
  core = {valid, invalid, reason, smtpStatusCode} = result
  log "EmailValidator #{context} #{email}: " + formattedInspect merge(core), 10000
  if !valid
    each value, key in result when !core[key]? && value is String
      log "EmailValidator #{context} #{email}:   #{key}: " + formattedInspect value.replace /\ *[\r\n]+[- .\d]*/g, ' '