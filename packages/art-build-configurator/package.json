  },
  "scripts": {
    "build": "caf -m -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "mocha -u tdd",
    "watch": "npm-watch"
  },
  "version": "1.28.2",
  "watch": {
    "build": {
      "extensions": [
        "caf"
      ],
      "patterns": [
        "source-source"
