{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-class-system": "^1.11.10",
    "art-events": "^1.3.7",
    "art-standard-lib": "^1.70.1",
    "chai": "^4.2.0",
    "neptune-namespaces-runtime": "^3.2.6",
    "npm-watch": "^0.7.0"
  },
  "description": "Art-Suite Testing tools. Right now, wrapper & extensions for Mocha & Chai",
  "devDependencies": {
    "art-build-configurator": "^1.28.0"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "art-testbench",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee --output source --compile --map source-source &\ncaf -p -c source-source -o source &\nwait\nnn -sq",
    "build-clean": "rm -rf source/*; npm run build",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "2.2.10",
  "watch": {
    "build": {
      "extensions": "caf,coffee",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
