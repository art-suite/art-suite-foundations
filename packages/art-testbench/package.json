{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-class-system": "^1.11.12",
    "art-events": "^1.3.9",
    "art-standard-lib": "^1.70.3",
    "chai": "^4.2.0",
    "neptune-namespaces-runtime": "^3.2.8",
    "npm-watch": "^0.7.0"
  },
  "description": "Art-Suite Testing tools. Right now, wrapper & extensions for Mocha & Chai",
  "devDependencies": {
    "art-build-configurator": "^1.28.3"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/_?[Jjt]est/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "art-testbench",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee -m --output source --compile source-source &\ncaf -m -c source-source -o source &\nwait\nnn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "3.1.0",
  "watch": {
    "build": {
      "extensions": [
        "caf",
        "coffee"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
