  "dependencies": {
    "art-build-configurator": "^1.26.11",
    "art-events": "^1.3.1",
    "chai": "^4.2.0"
  },
  "description": "Art-Suite Testing tools. Right now, wrapper & extensions for Mocha & Chai",
  "devDependencies": {},
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "license": "ISC",
  "name": "art-testbench",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee --output source --compile source-coffee-caf &\ncaf -p -c source-coffee-caf -o source &\nwait\nnn -s",
    "build-clean": "rm -rf source/*; npm run build",
    "start": "webpack-dev-server --hot --inline --progress --env.devServer",
    "test": "jest",
    "testInBrowser": "webpack-dev-server --progress --env.devServer",
    "watch": "watch 'npm run build' ./source-coffee-caf"
  },
  "version": "1.18.5"
}
