{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https://github.com/art-suite/art-suite-foundations/issues",
  "dependencies": {
    "art-binary": "^1.1.13",
    "art-standard-lib": "^1.70.5",
    "caffeine-script-runtime": "^1.14.0",
    "neptune-namespaces-runtime": "^3.2.8"
  },
  "description": "Art.Xbd",
  "devDependencies": {
    "art-build-configurator": "^1.29.2"
  },
  "homepage": "https://github.com/art-suite/art-suite-foundations",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/test/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "art-xbd",
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-foundations.git"
  },
  "scripts": {
    "build": "coffee --output source --compile source-source; caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "1.1.2",
  "watch": {
    "build": {
      "extensions": [
        "caf"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
